<!-- Riddhi-->

<table>
  <tr>
    <th>City Name</th>
  </tr>
  <%@cities.each do |city| %>
  <tr>
    <td><%=city.name%></td>
    <td><%=link_to "Edit",edit_city_path(city)%></td>
    <td><%=link_to "Delete",city,method: :Delete %></td>
  </tr>
  <%end%>
</table>
<%=link_to "Add New City" ,new_city_path%>
